	number = {20},
	journal = {Journal of Virology},
	author = {Lau, Susanna K. P. and Feng, Yun and Chen, Honglin and Luk, Hayes K. H. and Yang, Wei-Hong and Li, Kenneth S. M. and Zhang, Yu-Zhen and Huang, Yi and Song, Zhi-Zhong and Chow, Wang-Ngai and Fan, Rachel Y. Y. and Ahmed, Syed Shakeel and Yeung, Hazel C. and Lam, Carol S. F. and Cai, Jian-Piao and Wong, Samson S. Y. and Chan, Jasper F. W. and Yuen, Kwok-Yung and Zhang, Hai-Lin and Woo, Patrick C. Y.},
	month = oct,
