@article{liang_detection_2017,
	title = {Detection of diverse viruses in alimentary specimens of bats in {Macau}},
	volume = {32},
	issn = {1995-820X},
	doi = {10.1007/s12250-017-3976-9},
	abstract = {Bats carry a variety of viruses, and some of them cause public health problems. Macau, which is famous for its gambling industry, has a complex population structure. The globalization in such an international metropolis has enhanced the chance of disease transmission. Therefore, surveillance of zoonotic viruses is necessary for the early warning of potential emerging infectious diseases. Here, we report the first surveillance of bat viruses in Macau. In this study, we collected 1004 samples involving 10 bat species from 7 sites from April 2015 to May 2016, and examined the presence of viruses using nucleic acid-based methods. Coronaviruses, adenoviruses and paramyxoviruses were detected in these samples, with a high prevalence of coronaviruses. While, none was positive for hepatitis A virus, hepatitis E virus or hantavirus. Co-infections are not common in those bat species, but coronavirus HKU6 and adenovirus can be found commonly occurred in Myotis ricketti.},
	language = {eng},
	number = {3},
	journal = {Virologica Sinica},
	author = {Liang, Jie and Yang, Xing-Lou and Li, Bei and Liu, Qi and Zhang, Qin and Liu, Hui and Kan, Hon-Pio and Wong, Kai-Chin and Chek, Si-Nga and He, Xiangyang and Peng, Xingwen and Shi, Zheng-Li and Wu, Yi and Zhang, Libiao},
