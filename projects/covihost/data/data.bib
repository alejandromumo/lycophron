@article{vlasova_molecular_2011,
	title = {Molecular characterization of a new species in the genus {Alphacoronavirus} associated with mink epizootic catarrhal gastroenteritis},
	volume = {92},
	issn = {1465-2099},
	doi = {10.1099/vir.0.025353-0},
	abstract = {A coronavirus (CoV) previously shown to be associated with catarrhal gastroenteritis in mink (Mustela vison) was identified by electron microscopy in mink faeces from two fur farms in Wisconsin and Minnesota in 1998. A pan-coronavirus and a genus-specific RT-PCR assay were used initially to demonstrate that the newly discovered mink CoVs (MCoVs) were members of the genus Alphacoronavirus. Subsequently, using a random RT-PCR approach, full-genomic sequences were generated that further confirmed that, phylogenetically, the MCoVs belonged to the genus Alphacoronavirus, with closest relatedness to the recently identified but only partially sequenced (fragments of the polymerase, and full-length spike, 3c, envelope, nucleoprotein, membrane, 3x and 7b genes) ferret enteric coronavirus (FRECV) and ferret systemic coronavirus (FRSCV). The molecular data presented in this study provide the first genetic evidence for a new coronavirus associated with epizootic catarrhal gastroenteritis outbreaks in mink and demonstrate that MCoVs possess high genomic variability and relatively low overall nucleotide sequence identities (91.7 \%) between contemporary strains. Additionally, the new MCoVs appeared to be phylogenetically distant from human (229E and NL63) and other alphacoronaviruses and did not belong to the species Alphacoronavirus 1. It is proposed that, together with the partially sequenced FRECV and FRSCV, they comprise a new species within the genus Alphacoronavirus.},
	language = {eng},
	number = {Pt 6},
	journal = {The Journal of General Virology},
	author = {Vlasova, Anastasia N. and Halpin, Rebecca and Wang, Shiliang and Ghedin, Elodie and Spiro, David J. and Saif, Linda J.},
	month = jun,
	year = {2011},
	pmid = {21346029},
	pmcid = {PMC3168282},
	keywords = {Animals, Base Sequence, Coronavirus, Feces, Gastroenteritis, Genome, Viral, Mink, Molecular Sequence Data, Phylogeny, Viral Proteins, dwc-host: \{sciHost: Mustela vison, verHost: mink\}, dwc-host: \{verHost: bovine\}, dwc-host: \{verHost: cats\}, dwc-host: \{verHost: ferret\}, dwc-host: \{verHost: mouse\}, dwc-host: \{verHost: palm civets\}, dwc-host: \{verHost: porcine\}, dwc-host: \{verHost: turkey\}, dwc-relationship: dwc-host: \{sciHost: Mustela vison, verHost: mink\} dwc-virus: \{sciVirus: mink CoVs, abVirus: MCoVs\}, dwc-relationship: dwc-host: \{verHost: bovine\} dwc-virus: \{sciVirus: Bovine CoV, abVirus: BCoV\}, dwc-relationship: dwc-host: \{verHost: cats\} dwc-virus: \{abVirus: SARS-CoV\}, dwc-relationship: dwc-host: \{verHost: ferrets\} dwc-virus: \{abVirus: SARS-CoV\}, dwc-relationship: dwc-host: \{verHost: ferret\} dwc-virus: \{sciVirus: ferret enteric coronavirus, abVirus: FRECV\}, dwc-relationship: dwc-host: \{verHost: ferret\} dwc-virus: \{sciVirus: ferret systemic coronavirus, abVirus: FRSCV\}, dwc-relationship: dwc-host: \{verHost: mouse\} dwc-virus: \{sciVirus: mouse hepatitis virus, abVirus: MHV\}, dwc-relationship: dwc-host: \{verHost: palm civets\} dwc-virus: \{abVirus: SARS-CoV\}, dwc-relationship: dwc-host: \{verHost: porcine\} dwc-virus: \{sciVirus: Porcine respiratory coronavirus, abVirus: PRCV\}, dwc-relationship: dwc-host: \{verHost: turkey\} dwc-virus: \{sciVirus: Turkey coronavirus, abVirus: TCoV\}, dwc-virus: \{abVirus: SARS-CoV\}, dwc-virus: \{sciVirus: Bovine CoV, abVirus: BCoV\}, dwc-virus: \{sciVirus: Porcine respiratory coronavirus, abVirus: PRCV\}, dwc-virus: \{sciVirus: Turkey coronavirus, abVirus: TCoV\}, dwc-virus: \{sciVirus: ferret enteric coronavirus, abVirus: FRECV\}, dwc-virus: \{sciVirus: ferret systemic coronavirus, abVirus: FRSCV\}, dwc-virus: \{sciVirus: mink CoVs, abVirus: MCoVs\}, dwc-virus: \{sciVirus: mouse hepatitis virus, abVirus: MHV\}},
