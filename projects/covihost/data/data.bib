	keywords = {Animals, China, Chiroptera, Computational Biology, Coronavirus, Densovirinae, Dicistroviridae, Feces, High-Throughput Nucleotide Sequencing, Insect Viruses, Metagenomics, Molecular Sequence Data, Parvoviridae, Phylogeny, dwc-host: \{sciHost: Hipposideros, verHost: bat\}, dwc-relationship: dwc-host: \{sciHost: Hipposideros, verHost: bat\} dwc-virus: \{abVirus: BtCoV HKU9\}, dwc-virus: \{abVirus: BtCoV HKU9\}},
